var new_rels = [];

var next_custom_id = function() {
  var rel_ids = data.rels.map(function(r) { return r.id }).filter(function(r) { return r.id[0] == "x"; }).sort();
  last_id = (rel_ids.length == 0) ? "0" : rel_ids[rel_ids.length - 1];
  return "x" + (parseInt(last_id.slice(1)) + 1);
}

data.rels.forEach(function(rel, i) {
  rel.scale = 3;

  if (rel.category_id == 1) {
    rel.is_directional = false;
    rel.color = "#f0f";
    rel.fill_color = "#f0f";
  } else {
    rel.color = "#0a0";
    rel.fill_color = "#0f0";
  }

  rel.opacity = 0.8;
  rel.fill_opacity = 0.1;

  var matches = rel.label.match(/interlocks?/)

  if (rel.category_id == 5 && matches) {
    rel.label = 'Donation';
    new_rel = JSON.parse(JSON.stringify(rel));
    new_rel.id = next_custom_id();
    new_rel.category_id = 1;
    new_rel.category_ids = [1];
    new_rel.label = matches[0];
    new_rel.is_directional = false;
    //new_rel.color = "#00f";
    new_rels.push(rel);
    new_rels.push(new_rel);
  } else {
    new_rels.push(rel);
  }
});

data.rels = new_rels;

var sorted_entities = [];

data.entities.forEach(function(e, i) { 
  e.scale = 5;
  e.rel_count = data.rels.reduce(function(prev, r) {
    switch (r.category_id) {
      case 1:
      case 6:
        return prev + ((r.entity1_id == e.id || r.entity2_id == e.id) ? 2 : 0);
      case 5:
      case 10:
        return prev + (r.entity1_id == e.id ? 1 : 0); 
      default:
        return prev;
    }
  }, 0);
});

sorted_entities = data.entities.sort(function(a, b) {
  return b.rel_count - a.rel_count;
});

sorted_entities.forEach(function(e, i) {
  e.color = 'none';
});

data.entities = sorted_entities;

